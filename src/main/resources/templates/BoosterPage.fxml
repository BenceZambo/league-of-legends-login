<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.BorderPane?>
<?scenebuilder-stylesheet ../BoosterPage.css?>

<BorderPane prefHeight="400.0" prefWidth="600.0" style="-fx-background-color: skyblue;" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1">
   <center>
      <TableView fx:id="table" stylesheets="/css/TableView.css" BorderPane.alignment="CENTER">
        <columns>
          <TableColumn fx:id="id_column" text="Id" />
          <TableColumn fx:id="purchase_column" prefWidth="436.0" text="Purchase" />
            <TableColumn fx:id="server_column" text="Server" />
            <TableColumn fx:id="status_column" text="Status" />
        </columns>
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
      </TableView>
   </center>
   <bottom>
      <Button fx:id="launchButton" mnemonicParsing="false" prefHeight="71.0" prefWidth="110.0" stylesheets="/css/LaunchButton.css" text="Launch" textFill="#eee9e9" BorderPane.alignment="CENTER" />
   </bottom>
   <top>
      <BorderPane prefWidth="200.0" BorderPane.alignment="CENTER">
         <left>
            <Button fx:id="reFresh" alignment="CENTER" mnemonicParsing="false" prefHeight="42.0" prefWidth="124.0" stylesheets="/css/ReFresh.css" BorderPane.alignment="CENTER">
             <graphic>
                 <ImageView fitHeight="34.0" fitWidth="124.0" preserveRatio="true">
                     <image>
                         <Image url="@../pictures/reload-icon.jpg" />
                     </image>
                 </ImageView>
             </graphic>
         </Button>
         </left>
         <right>
            <Button fx:id="signOut" mnemonicParsing="false" prefHeight="42.0" prefWidth="124.0" stylesheets="/css/SignOut.css" text="Sign out" BorderPane.alignment="CENTER" />
         </right>
      </BorderPane>
   </top>
</BorderPane>
